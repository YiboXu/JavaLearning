        <td id="LC273" class="blob-code blob-code-inner js-file-line">{ <span class="pl-s"><span class="pl-pds">&quot;</span>_id<span class="pl-pds">&quot;</span></span> : <span class="pl-c1">514</span>, <span class="pl-s"><span class="pl-pds">&quot;</span>title<span class="pl-pds">&quot;</span></span> : <span class="pl-s"><span class="pl-pds">&quot;</span>Android in Action, Third Edition<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>isbn<span class="pl-pds">&quot;</span></span> : <span class="pl-s"><span class="pl-pds">&quot;</span>1617290505<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>pageCount<span class="pl-pds">&quot;</span></span> : <span class="pl-c1">0</span>, <span class="pl-s"><span class="pl-pds">&quot;</span>publishedDate<span class="pl-pds">&quot;</span></span> : { <span class="pl-s"><span class="pl-pds">&quot;</span>$date<span class="pl-pds">&quot;</span></span> : <span class="pl-s"><span class="pl-pds">&quot;</span>2011-11-15T00:00:00.000-0800<span class="pl-pds">&quot;</span></span> }, <span class="pl-s"><span class="pl-pds">&quot;</span>thumbnailUrl<span class="pl-pds">&quot;</span></span> : <span class="pl-s"><span class="pl-pds">&quot;</span>https://s3.amazonaws.com/AKIAJC5RLADLUMVRPFDQ.book-thumb-images/ableson3.jpg<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>status<span class="pl-pds">&quot;</span></span> : <span class="pl-s"><span class="pl-pds">&quot;</span>PUBLISH<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>authors<span class="pl-pds">&quot;</span></span> : [ <span class="pl-s"><span class="pl-pds">&quot;</span>W. Frank Ableson<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>Robi Sen<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>Chris King<span class="pl-pds">&quot;</span></span>, <span class="pl-s"><span class="pl-pds">&quot;</span>C. Enrique Ortiz<span class="pl-pds">&quot;</span></span> ], <span class="pl-s"><span class="pl-pds">&quot;</span>categories<span class="pl-pds">&quot;</span></span> : [] }</td>
